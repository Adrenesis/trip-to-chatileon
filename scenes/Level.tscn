code = "shader_type canvas_item;

uniform sampler2D wave_pattern;
uniform vec4 water_tint_0;

void fragment() {
	float uvx = mod((UV.x-0.5)*(5.0/((UV.y)*10.0+1.0))*3.0+TIME/10.0+sin(TIME/5.0)/4.0, 1.0);
	float uvy = mod(UV.y*6.0+TIME/50.0+sin(TIME/5.0)/4.0, 1.0);
	vec4 color = textureLod(wave_pattern, vec2(uvx, uvy), 1.0);
	color = mix(color, textureLod(wave_pattern, mod(vec2(UV.x, UV.y)+TIME/64.0, 1.0), 1.0), 0.4);
	color = mix(color, water_tint_0, 0.8);
	COLOR = color;
}"

